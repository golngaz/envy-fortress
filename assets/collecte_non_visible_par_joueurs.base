views:
  - type: table
    name: Table
    filters:
      and:
        - joueur != "visible"
        - file.ext == "md"
        - '!file.folder.startsWith("LORE")'
    order:
      - file.name
      - file.folder
      - file.ctime
      - file.ext
      - joueur
    sort:
      - property: file.folder
        direction: ASC
